{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mm0",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_sort_stmt"
        },
        {
          "type": "SYMBOL",
          "name": "_term_stmt"
        }
      ]
    },
    "_sort_stmt": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "sort"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "_term_stmt": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "term"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_type_binder"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_arrow_type"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "_type_binder": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "SYMBOL",
              "name": "type"
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_identifier_"
              }
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "SYMBOL",
              "name": "type"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "_arrow_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "type"
            },
            {
              "type": "STRING",
              "value": ">"
            },
            {
              "type": "SYMBOL",
              "name": "_arrow_type"
            }
          ]
        }
      ]
    },
    "type": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "_identifier_": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "_"
        }
      ]
    },
    "_lexeme": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_symbol"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "_math_string"
        }
      ]
    },
    "_symbol": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "STRING",
          "value": ")"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": "_"
        }
      ]
    },
    "_math_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "math_lexeme"
        },
        {
          "type": "SYMBOL",
          "name": "_math_whitespace"
        }
      ]
    },
    "_math_whitespace": {
      "type": "PATTERN",
      "value": "( |\\n)*"
    },
    "math_lexeme": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "math_lexeme"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "0|[1-9][0-9]*"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z]*"
    },
    "_whitestuff": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_whitechar"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "_whitechar": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "comment": {
      "type": "SYMBOL",
      "name": "_line_comment"
    },
    "_line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "--"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*\\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}